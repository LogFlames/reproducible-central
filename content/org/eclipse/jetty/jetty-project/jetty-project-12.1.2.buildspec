command="mvn -Peclipse-release clean package -DskipTests -Dgpg.skip -Dtimestamp=${timestamp} -Dbuildinfo.ignore='*/*-demo-jspc-*.war,*/*-demo-jspc-*webapp-${version}-webbundle-*.jar,*/*-p2site.zip'"

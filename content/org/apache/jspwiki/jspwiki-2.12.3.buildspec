execBefore="chmod a-x jspwiki-it-tests/jspwiki-selenide-tests/src/main/java/org/apache/wiki/its/WithIntegrationTestSetup.java jspwiki-main/src/test/java/org/apache/wiki/plugin/DenounceTest.java jspwiki-portable/src/overlay/launchers/tomcat/woas.app/Contents/MacOS/JavaAppLauncher jspwiki-portable/src/overlay/launchers/tomcat/woas.sh jspwiki-portable/src/resources/macos/JavaApplicationStub"

diffoscope=${artifactId}-${version}.diffoscope
issue=
